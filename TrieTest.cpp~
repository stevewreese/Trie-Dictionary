
#include "Trie.h"
#include <iostream>


int main(){
    Trie test;
    std::string b = "no";
    test.addWord("word");
    if(test.isWord("word")){
      b =  "yes";
    }

    std::cout << b << std::endl;
    std::vector<std::string> testvect;
    testvect = test.allWordsWithPrefix("word");
    std::cout << testvect[0] << std::endl;
}
